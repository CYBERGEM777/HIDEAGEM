    <PostBuildEvent>
      <Command>@echo off
SETLOCAL

:: Change to the base directory relative to the project directory
pushd ..\..\..\

:: Define the source and target directories
SET source_dir=extras\ComfyUI
SET target_dir=builds\temp\HIDEAGEM
SET bin_dir=bin
SET zip_file=%bin_dir%\HIDEAGEM_Win64.zip

:: Create the target directory
if not exist "%target_dir%" mkdir "%target_dir%"

:: Copy files and directory contents
xcopy /E /I "%source_dir%\*" "%target_dir%"
copy "LICENSE" "%target_dir%"
copy "README.md" "%target_dir%"
copy "HIDEAGEM.py" "%target_dir%"
copy "requirements.txt" "%target_dir%"
copy "%bin_dir%\HIDEAGEM.dll" "%target_dir%"

:: Check if the zip file already exists and delete it if it does
if exist "%zip_file%" del /F /Q "%zip_file%"

:: Change directory to the builds\temp directory
pushd builds\temp

:: Zip the directory
powershell -Command "Compress-Archive -Path 'HIDEAGEM' -DestinationPath '..\..\%zip_file%'"

:: Return to the original directory
popd

:: Delete the HIDEAGEM_ComfyUI_Win64 directory
rd /s /q "%target_dir%"

:: End of script
ENDLOCAL
echo Done
</Command>
